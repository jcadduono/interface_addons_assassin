<Bindings>
	<Binding name="ASSASSIN_TARGETMORE" category="BINDING_CATEGORY_ASSASSIN">
		Assassin_ToggleTargetMode()
	</Binding>
	<Binding name="ASSASSIN_TARGETLESS" category="BINDING_CATEGORY_ASSASSIN">
		Assassin_ToggleTargetModeReverse()
	</Binding>
	<Binding name="ASSASSIN_TARGET1" category="BINDING_CATEGORY_ASSASSIN">
		Assassin_SetTargetMode(1)
	</Binding>
	<Binding name="ASSASSIN_TARGET2" category="BINDING_CATEGORY_ASSASSIN">
		Assassin_SetTargetMode(2)
	</Binding>
	<Binding name="ASSASSIN_TARGET3" category="BINDING_CATEGORY_ASSASSIN">
		Assassin_SetTargetMode(3)
	</Binding>
	<Binding name="ASSASSIN_TARGET4" category="BINDING_CATEGORY_ASSASSIN">
		Assassin_SetTargetMode(4)
	</Binding>
	<Binding name="ASSASSIN_TARGET5" category="BINDING_CATEGORY_ASSASSIN">
		Assassin_SetTargetMode(5)
	</Binding>
</Bindings>
