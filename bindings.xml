<Bindings>
	<Binding name="Toggle Targets +" header="ASSASSIN" category="ADDONS">
		Assassin_ToggleTargetMode()
	</Binding>
	<Binding name="Toggle Targets -" category="ADDONS">
		Assassin_ToggleTargetModeReverse()
	</Binding>
	<Binding name="Set Targets to 1" category="ADDONS">
		Assassin_SetTargetMode(1)
	</Binding>
	<Binding name="Set Targets to 2" category="ADDONS">
		Assassin_SetTargetMode(2)
	</Binding>
	<Binding name="Set Targets to 3" category="ADDONS">
		Assassin_SetTargetMode(3)
	</Binding>
	<Binding name="Set Targets to 4" category="ADDONS">
		Assassin_SetTargetMode(4)
	</Binding>
	<Binding name="Set Targets to 5+" category="ADDONS">
		Assassin_SetTargetMode(5)
	</Binding>
</Bindings>
